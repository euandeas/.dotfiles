{
  "edit_predictions": {
    "mode": "subtle",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null,
      "enterprise_uri": null
    },
    "enabled_in_text_threads": false
  },
  "agent": {
    "default_profile": "ask",
    "model_parameters": [],
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4o"
    }
  },
  "icon_theme": "Colored Zed Icons Theme Dark",
  "base_keymap": "VSCode",
  "features": {
    "edit_prediction_provider": "zed"
  },
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "buffer_font_size": 14,
  "buffer_font_family": "AdwaitaMono Nerd Font",
  "theme": {
    "mode": "system",
    "light": "Flexoki Light",
    "dark": "Flexoki Dark"
  },
  "lsp": {
    "tinymist": {
      "settings": {
        "exportPdf": "onSave",
        "outputPath": "$root/$name"
      }
    }
  },
  "soft_wrap": "editor_width"
}
